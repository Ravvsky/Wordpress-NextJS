{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "public/*": [
        "../public/*"
      ]
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/models/classname.tsc", "src/pages/index.js", "src/pages/[slugParent]/[[...slugChild]].js", "src/pages/_document.js", "src/pages/404.js"  ],
  "exclude": [
    "node_modules"
  ]
}
